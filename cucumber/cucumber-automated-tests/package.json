{
    "name": "cucumber",
    "version": "1.0.0",
    "description": "test automation with cucumber on javascript",
    "main": "index.js",
    "scripts": {
      "test": "wdio run ./wdio.conf.js",
      "test:regression":"wdio run ./wdio.conf.js --cucumberOpts.tagExpression='@regression'",
      "test:dev":"wdio run ./wdio.conf.js --cucumberOpts.tagExpression='@dev'",
      "test:int":"wdio run ./wdio.conf.js --cucumberOpts.tagExpression='@int'",
      "wdio": "wdio run ./wdio.conf.js"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/digracesion/automation-learning/cucumber/cucumber-automated-tests.git"
    },
    "keywords": [
      "cucumber",
      "automation"
    ],
    "author": "digracesion",
    "license": "ISC",
    "devDependencies": {
      "@babel/preset-env": "^7.20.2",
      "@babel/register": "^7.21.0",
      "@wdio/cli": "^8.5.1",
      "@wdio/cucumber-framework": "^8.4.0",
      "@wdio/local-runner": "^8.5.1",
      "@wdio/selenium-standalone-service": "^8.5.1",
      "@wdio/spec-reporter": "^8.4.0",
      "chromedriver": "^110.0.0",
      "wdio-chromedriver-service": "^8.1.1"
    },
    "dependencies": {
      "babel-register": "^6.26.0",
      "npm": "^9.5.1"
    }
  }
  